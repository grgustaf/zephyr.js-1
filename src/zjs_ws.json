{
    "module": "ws",
    "require": "ws",
    "depends": ["buffer", "events", "net_config_default"],
    "virtualdeps": ["net-l2"],
    "zephyr_conf": {
        "all": [
            "CONFIG_NETWORKING=y",
            "CONFIG_NET_IPV6=y",
            "CONFIG_NET_IPV4=y",
            "CONFIG_NET_TCP=y",
            "CONFIG_TEST_RANDOM_GENERATOR=y",
            "CONFIG_INIT_STACKS=y",
            "CONFIG_PRINTK=y",
            "CONFIG_NET_STATISTICS=y",
            "CONFIG_NET_BUF=y",
            "CONFIG_NET_PKT_RX_COUNT=5",
            "CONFIG_NET_PKT_TX_COUNT=5",
            "CONFIG_NET_BUF_RX_COUNT=10",
            "CONFIG_NET_BUF_TX_COUNT=6",
            "CONFIG_NET_IF_UNICAST_IPV6_ADDR_COUNT=3",
            "CONFIG_NET_IF_MCAST_IPV6_ADDR_COUNT=2",
            "CONFIG_NET_MAX_CONTEXTS=10",
            "CONFIG_NET_MGMT=y",
            "CONFIG_NET_MGMT_EVENT=y",
            "CONFIG_MBEDTLS=y",
            "CONFIG_MBEDTLS_BUILTIN=y",
            "CONFIG_MBEDTLS_CFG_FILE=\"$(ZJS_BASE)/src/zjs_mbedtls_config.h\""
        ]
    },
    "src": ["zjs_web_sockets.c"],
    "zjs_config": ["-DBUILD_MODULE_WS"],
    "header": ["zjs_web_sockets.h"],
    "init": ["zjs_ws_init"],
    "cleanup": ["zjs_ws_cleanup"]
}
